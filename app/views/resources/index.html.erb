<h2 class='text-info'>Index</h2>
<br>

 <section class='project-related'>
   <h3 class="text-info"><u>Project Related</u></h3>
   <br>
   <% project_related_category(@resources).each do |resource| %>
   <section id="resource-<%= resource.id %>">
     <p><b>Link:</b> <%= link_to "#{resource.title}", resource.link %>
     <p><b>Website:</b> <%= resource.source %>
     <p><b>Category:</b> <%= resource.category %>
       <% if resource.project.empty? == false %>
         <p><b>Related Project (if applicable):</b> <%= resource.project %>
       <% end %>
     <p><%= link_to 'Edit Resource', "/resources/#{resource.id}/edit", method: :get, class: 'btn btn-info btn-xs' %>
     <%= link_to 'Delete Resource', "/resources/#{resource.id}", method: :delete, class: 'btn btn-info btn-xs' %>
    </section>
   <% end %>
   <br>
 </section>

